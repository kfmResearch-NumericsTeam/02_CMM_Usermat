*DIM,fsy2,ARRAY,n_F
*DIM,fsu2,ARRAY,n_F
*DIM,esu2,ARRAY,n_F
*DIM,esv2,ARRAY,n_F
*DIM,Es2,ARRAY,n_F
*DIM,ka2,ARRAY,n_F
*DIM,kb2,ARRAY,n_F
*DIM,kc2,ARRAY,n_F
*DIM,as2,ARRAY,n_F
*DIM,dm2,ARRAY,n_F
*DIM,psi2,ARRAY,n_F
!
