! Auswahl Analyse Art
nlfe=1                              ! '0': Elastische Analyse
                                    ! '1': NLFE-Analyse
!
! Auswahl Analyse Dimension
Dimens = 2                          ! '1': 2D (Plane 182)
                                    ! '2': 3D (SHELL 181)                                    
! Auswahl Druckfeldmodell
