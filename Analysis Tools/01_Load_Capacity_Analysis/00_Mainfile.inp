*DIM,fsy4,ARRAY,n_F
*DIM,fsu4,ARRAY,n_F
*DIM,esu4,ARRAY,n_F
*DIM,esv4,ARRAY,n_F
*DIM,Es4,ARRAY,n_F
*DIM,ka4,ARRAY,n_F
*DIM,kb4,ARRAY,n_F
*DIM,kc4,ARRAY,n_F
*DIM,as4,ARRAY,n_F
*DIM,dm4,ARRAY,n_F
*DIM,psi4,ARRAY,n_F     
!
! 2.3.2 Zuordnung der Werkstoffparameter f�r alle Ansys Fl�chen 
! -------------------------------------------------------------
!
f_sy = fsy_P                        ! Fliesspannung/Beiwert [N/mm2] 
f_su = fsu_P                        ! Bruchspannung/Beiwert [N/mm2]  
!
*do,ii,1,n_F            
!
    ! 1. Lage
    stahl1(ii) = 1                  ! '1': bilin.,'2': kv,'3': wv
    zm1(ii) = 2                     ! '1': bilin.,'2': bl_seel,'3': kv, '4': wv
    fsy1(ii) = f_sy                 ! Fliesspannung/Beiwert [N/mm2] 
    fsu1(ii) = f_su                 ! Bruchspannung/Beiwert [N/mm2]  
    esu1(ii) = 0.15                ! Bruchverzerrung*Beiwert [-]
    esv1(ii) = 0.018                ! F�r bilin. nicht ben�tigt [-]
    Es1(ii) = 200000                ! Elastizit�tsmodul [N/mm2] 
    ka1(ii) = -1                    ! F�r bilin. nicht ben�tigt [-]
    kb1(ii) = -1                    ! F�r bilin. nicht ben�tigt [-]
    kc1(ii) = -1                    ! F�r bilin. nicht ben�tigt [-]
    as1(ii) = 2 
    dm1(ii) = 20
    psi1(ii) = 90
!            
    ! 2. Lage
    stahl2(ii) = 1              
    zm2(ii) = 2                 
    fsy2(ii) = f_sy         
    fsu2(ii) = f_su          
    esu2(ii) = 0.15            
    esv2(ii) = 0.018                
    Es2(ii) = 200000            
    ka2(ii) = -1                
    kb2(ii) = -1                
    kc2(ii) = -1 
    as2(ii) = 2
    dm2(ii) = 20
    psi2(ii) = 0              
!
    ! 3. Lage
    stahl3(ii) = 1              
    zm3(ii) = 2                 
    fsy3(ii) = f_sy          
    fsu3(ii) = f_su          
    esu3(ii) = 0.15            
    esv3(ii) = 0.018                
    Es3(ii) = 200000            
    ka3(ii) = -1                
    kb3(ii) = -1                
    kc3(ii) = -1
    as3(ii) = 2 
    dm3(ii) = 20
    psi3(ii) = 0                      
!
    ! 4. Lage
    stahl4(ii) = 1             
    zm4(ii) = 2                 
    fsy4(ii) = f_sy          
    fsu4(ii) = f_su          
    esu4(ii) = 0.15            
    esv4(ii) = 0.018                
    Es4(ii) = 200000            
    ka4(ii) = -1                
    kb4(ii) = -1                
    kc4(ii) = -1 
    as4(ii) = 2 
    dm4(ii) = 20
